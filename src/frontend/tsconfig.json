{
  "extends": "../../tsconfig.frontend.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/stores/*": [
        "./stores/*"
      ],
      "@/features/*": [
        "./features/*"
      ],
      "@/styles/*": [
        "./styles/*"
      ],
      "@shared/*": [
        "../../src/shared/*"
      ]
    },
    "allowJs": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
